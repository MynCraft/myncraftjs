{
  "name": "myncraftjs",
  "version": "0.0.5",
  "description": "Simple minecraft bot package extends from mineflayer",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "types": "./dist/typings/index.d.ts",
  "scripts": {
    "dev": "nodemon --watch ./src -e ts --exec 'npm run test' ",
    "test": "clear&&cd src&&node --no-warnings=ExperimentalWarning --loader ts-node/esm test.ts",
    "build": "echo 'Build started'&&npx tsc&&npx tsc -p tsconfig.mjs.json&&echo 'Build finished'"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myncraft/MynCraftjs.git"
  },
  "keywords": [
    "mineflayer",
    "minecraft",
    "myncraftjs",
    "mcbot",
    "minecraftbot",
    "mineflayerjs",
    "senesatka",
    "myncraft",
    "bot",
    "mcbot",
    "@myncraftjs",
    "@myncraft",
    "senka"
  ],
  "author": {
    "email": "senesatka@gmail.com",
    "name": "SeneSatka"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MynCraft/myncraftjs/issues"
  },
  "homepage": "https://github.com/MynCraft/myncraftjs#readme",
  "dependencies": {
    "@myncraftjs/colorchat": "^1.0.3",
    "@myncraftjs/logger": "^1.0.0",
    "@myncraftjs/spawncommands": "^1.0.0",
    "minecraft-protocol": "^1.57.0",
    "mineflayer": "^4.29.0",
    "prismarine-entity": "^2.5.0",
    "prismarine-item": "^1.16.0",
    "typed-emitter": "^1.4.0",
    "vec3": "^0.1.10"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
